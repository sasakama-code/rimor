    /**
     * AI JSONÁîüÊàê (Issue #58)
     * UnifiedAnalysisResult„ÇíAIÂêë„ÅëJSONÂΩ¢Âºè„Å´Â§âÊèõ„Åó„Å¶Âá∫Âäõ
     */
    async generateAIJson(analysisResult, options) {
        try {
            // Á∞°ÊòìÂÆüË£Ö: ÂàÜÊûêÁµêÊûú„Åã„ÇâÁõ¥Êé•AI JSON„ÇíÁîüÊàê
            const issueCount = analysisResult?.issues?.length || 0;
            const score = issueCount === 0 ? 100 : Math.max(0, 100 - issueCount * 10);
            const grade = score >= 90 ? 'A' : score >= 80 ? 'B' : score >= 70 ? 'C' : score >= 60 ? 'D' : 'F';
            const aiJson = {
                overallAssessment: `„Éó„É≠„Ç∏„Çß„ÇØ„ÉàÂìÅË≥™Ë©ï‰æ°ÁµêÊûú:\nÁ∑èÂêà„Çπ„Ç≥„Ç¢: ${score}/100\n„Ç∞„É¨„Éº„Éâ: ${grade}\n\nÊ§úÂá∫„Åï„Çå„ÅüÂïèÈ°å: ${issueCount}‰ª∂`,
                keyRisks: (analysisResult?.issues?.slice(0, 10).map((issue) => {
                    const risk = {
                        problem: issue.message || 'ÂïèÈ°å„ÅåÊ§úÂá∫„Åï„Çå„Åæ„Åó„Åü',
                        riskLevel: this.mapSeverityToRiskLevel(issue.severity),
                        context: {
                            filePath: issue.file || 'unknown',
                            codeSnippet: '',
                            startLine: issue.line || 0,
                            endLine: issue.line || 0
                        },
                        suggestedAction: {
                            type: 'ADD_MISSING_TEST',
                            description: '„ÉÜ„Çπ„Éà„ÇíËøΩÂä†„Åó„Å¶„Åè„Å†„Åï„ÅÑ',
                            example: ''
                        }
                    };
                    return risk;
                }) || []),
                fullReportUrl: options.outputHtml || '.rimor/reports/index.html'
            };
            // Âá∫Âäõ
            if (options.outputAiJson) {
                const outputPath = path.resolve(options.outputAiJson);
                const outputDir = path.dirname(outputPath);
                if (!fs.existsSync(outputDir)) {
                    fs.mkdirSync(outputDir, { recursive: true });
                }
                fs.writeFileSync(outputPath, JSON.stringify(aiJson, null, 2));
                console.log(await output_1.OutputFormatter.success(`AI JSON„É¨„Éù„Éº„Éà„ÇíÁîüÊàê„Åó„Åæ„Åó„Åü: ${outputPath}`));
            }
            else {
                // „Ç≥„É≥„ÇΩ„Éº„É´Âá∫Âäõ
                console.log(JSON.stringify(aiJson, null, 2));
            }
        }
        catch (error) {
            // „Ç®„É©„Éº„ÅÆË©≥Á¥∞„ÇíÂá∫Âäõ
            const errorMessage = error instanceof Error ? error.message : String(error);
            console.error(await output_1.OutputFormatter.error(`AI JSONÁîüÊàê„Å´Â§±Êïó„Åó„Åæ„Åó„Åü: ${errorMessage}`));
            // „Çπ„Çø„ÉÉ„ÇØ„Éà„É¨„Éº„Çπ„ÇÇÂá∫ÂäõÔºàverbose„É¢„Éº„ÉâÊôÇÔºâ
            if (options.verbose && error instanceof Error) {
                console.error('Ë©≥Á¥∞:', error.stack);
            }
            // „Ç®„É©„Éº„Ç≥„Éº„Éâ1„ÅßÁµÇ‰∫Ü
            process.exit(1);
        }
    }
    /**
     * „Ç¢„Éé„ÉÜ„Éº„Ç∑„Éß„É≥ÁîüÊàêÂá¶ÁêÜ
     */
    async generateAnnotations(analysisResult, reporter, options) {
        // StructuredReporterImpl„ÅÆgenerateAnnotations„É°„ÇΩ„ÉÉ„Éâ„Çí‰ΩøÁî®
        if ('generateAnnotations' in reporter) {
            const annotationOptions = {
                outputDir: options.annotateOutput,
                overwrite: false,
                preview: options.preview,
                format: options.annotateFormat || 'inline',
                includeDataFlow: options.includeDetails
            };
            const result = await reporter.generateAnnotations(analysisResult, annotationOptions);
            if (result.success) {
                if (options.preview) {
                    console.log(await output_1.OutputFormatter.header("„Ç¢„Éé„ÉÜ„Éº„Ç∑„Éß„É≥„Éó„É¨„Éì„É•„Éº"));
                    if (reporter.printToConsole) {
                        reporter.printToConsole(result.content);
                    }
                }
                else {
                    console.log(await output_1.OutputFormatter.success("„Ç¢„Éé„ÉÜ„Éº„Ç∑„Éß„É≥„ÇíÁîüÊàê„Åó„Åæ„Åó„Åü"));
                    if (reporter.printToConsole) {
                        reporter.printToConsole(result.content);
                    }
                }
            }
            else {
                console.error(await output_1.OutputFormatter.error(`„Ç¢„Éé„ÉÜ„Éº„Ç∑„Éß„É≥ÁîüÊàê„Å´Â§±Êïó„Åó„Åæ„Åó„Åü: ${result.error}`));
            }
        }
        else {
            console.error(await output_1.OutputFormatter.error("„Åì„ÅÆ„Éê„Éº„Ç∏„Éß„É≥„ÅÆReporter„ÅØ„Ç¢„Éé„ÉÜ„Éº„Ç∑„Éß„É≥ÁîüÊàê„Çí„Çµ„Éù„Éº„Éà„Åó„Å¶„ÅÑ„Åæ„Åõ„Çì"));
        }
    }
    /**
     * TaintËß£ÊûêÁµêÊûú„ÇíÂèñÂæó
     * @param targetPath ÂàÜÊûêÂØæË±°„Éë„Çπ
     * @param analysisResult Êó¢Â≠ò„ÅÆÂàÜÊûêÁµêÊûú
     * @returns TaintAnalysisResult
     */
    async getTaintAnalysisResult(targetPath, analysisResult) {
        try {
            // Êó¢Â≠ò„ÅÆÂàÜÊûêÁµêÊûú„Åã„ÇâTaintÈñ¢ÈÄ£„ÅÆÊÉÖÂ†±„ÇíÊäΩÂá∫
            if (analysisResult?.pluginResults?.['taint-analysis']) {
                // Êó¢„Å´TaintËß£Êûê„ÅåÂÆüË°å„Åï„Çå„Å¶„ÅÑ„ÇãÂ†¥Âêà„ÅØ„Åù„ÅÆÁµêÊûú„Çí‰ΩøÁî®
                return this.convertToTaintAnalysisResult(analysisResult.pluginResults['taint-analysis']);
            }
            // ÂàÜÊûêÁµêÊûú„Åã„Çâissues„ÇíÂü∫„Å´TaintAnalysisResult„ÇíÁîüÊàê
            // ÂÆüÈöõ„ÅÆTaintËß£Êûê„ÅØÈáç„ÅÑ„Åü„ÇÅ„ÄÅÊó¢Â≠ò„ÅÆÂàÜÊûêÁµêÊûú„Åã„ÇâÂ§âÊèõ„Åô„Çã
            if (analysisResult?.issues && Array.isArray(analysisResult.issues)) {
                return this.convertIssuesToTaintResult(analysisResult.issues);
            }
            // „Éï„Ç©„Éº„É´„Éê„ÉÉ„ÇØ: „Éá„Éï„Ç©„É´„ÉàÂÄ§„ÇíËøî„Åô
            return this.getDefaultTaintResult();
        }
        catch (error) {
            // „Ç®„É©„ÉºÊôÇ„ÅØ„Éï„Ç©„Éº„É´„Éê„ÉÉ„ÇØ
            console.warn('TaintËß£ÊûêÁµêÊûú„ÅÆÂèñÂæó„Å´Â§±Êïó„Åó„Åæ„Åó„Åü„ÄÇ„Éá„Éï„Ç©„É´„ÉàÂÄ§„Çí‰ΩøÁî®„Åó„Åæ„Åô„ÄÇ', error);
            return this.getDefaultTaintResult();
        }
    }
    /**
     * „Éá„Éï„Ç©„É´„Éà„ÅÆTaintAnalysisResult„ÇíËøî„Åô
     */
    getDefaultTaintResult() {
        return {
            flows: [],
            summary: {
                totalFlows: 0,
                criticalFlows: 0,
                highFlows: 0,
                mediumFlows: 0,
                lowFlows: 0,
                sourcesCount: 0,
                sinksCount: 0,
                sanitizersCount: 0
            },
            recommendations: []
        };
    }
    /**
     * ÂàÜÊûêÁµêÊûú„ÅÆissues„ÇíTaintAnalysisResultÂΩ¢Âºè„Å´Â§âÊèõ
     */
    convertIssuesToTaintResult(issues) {
        // „Çª„Ç≠„É•„É™„ÉÜ„Ç£Èñ¢ÈÄ£„ÅÆissue„ÇíTaint„Éï„É≠„Éº„Å®„Åó„Å¶Êâ±„ÅÜ
        const securityIssues = issues.filter(issue => issue.type?.includes('security') ||
            issue.type?.includes('taint') ||
            issue.type?.includes('injection') ||
            issue.type?.includes('xss') ||
            issue.type?.includes('sql') ||
            issue.severity === 'critical' ||
            issue.severity === 'high');
        const flows = securityIssues.map((issue, index) => ({
            id: `flow-${index}`,
            source: 'user-input',
            sink: this.inferSinkFromIssue(issue),
            taintLevel: this.mapSeverityToLevel(issue.severity),
            path: [],
            confidence: 0.8,
            location: {
                file: issue.file || 'unknown',
                line: issue.line || 0
            }
        }));
        const summary = {
            totalFlows: flows.length,
            criticalFlows: flows.filter(f => f.taintLevel === 'critical').length,
            highFlows: flows.filter(f => f.taintLevel === 'high').length,
            mediumFlows: flows.filter(f => f.taintLevel === 'medium').length,
            lowFlows: flows.filter(f => f.taintLevel === 'low').length,
            sourcesCount: flows.length > 0 ? 1 : 0,
            sinksCount: new Set(flows.map(f => f.sink)).size,
            sanitizersCount: 0
        };
        return { flows, summary, recommendations: [] };
    }
    /**
     * issue„Åã„Çâsink„ÅÆ„Çø„Ç§„Éó„ÇíÊé®Ë´ñ
     */
    inferSinkFromIssue(issue) {
        const type = issue.type?.toLowerCase() || '';
        if (type.includes('sql'))
            return 'database';
        if (type.includes('xss'))
            return 'html-output';
        if (type.includes('command'))
            return 'shell-command';
        if (type.includes('file'))
            return 'file-system';
        return 'unknown-sink';
    }
    /**
     * „Éó„É©„Ç∞„Ç§„É≥ÁµêÊûú„ÇíTaintAnalysisResultÂΩ¢Âºè„Å´Â§âÊèõ
     */
    convertToTaintAnalysisResult(pluginResults) {
        // „Éó„É©„Ç∞„Ç§„É≥ÁµêÊûú„Åã„ÇâTaintAnalysisResultÂΩ¢Âºè„Å∏„ÅÆÂ§âÊèõ
        const flows = pluginResults.detections?.map((detection) => ({
            id: detection.patternId,
            source: detection.metadata?.source || 'unknown',
            sink: detection.metadata?.sink || 'unknown',
            taintLevel: this.mapSeverityToLevel(detection.severity),
            path: [],
            confidence: 0.8,
            location: detection.location
        })) || [];
        const summary = {
            totalFlows: flows.length,
            criticalFlows: flows.filter((f) => f.taintLevel === 'critical').length,
            highFlows: flows.filter((f) => f.taintLevel === 'high').length,
            mediumFlows: flows.filter((f) => f.taintLevel === 'medium').length,
            lowFlows: flows.filter((f) => f.taintLevel === 'low').length,
            sourcesCount: new Set(flows.map((f) => f.source)).size,
            sinksCount: new Set(flows.map((f) => f.sink)).size,
            sanitizersCount: 0
        };
        return { flows, summary, recommendations: [] };
    }
    /**
     * „Éó„É≠„Ç∏„Çß„ÇØ„ÉàËß£ÊûêÁµêÊûú„ÇíTaintAnalysisResultÂΩ¢Âºè„Å´Â§âÊèõ
     */
    convertProjectAnalysisToTaintResult(projectAnalysis) {
        // „Éó„É≠„Ç∏„Çß„ÇØ„ÉàËß£ÊûêÁµêÊûú„Åã„Çâ„ÅÆÂ§âÊèõ„É≠„Ç∏„ÉÉ„ÇØ
        const flows = projectAnalysis.issues?.map((issue) => ({
            id: `flow-${Math.random()}`,
            source: 'user-input',
            sink: 'database',
            taintLevel: this.mapSeverityToLevel(issue.severity),
            path: [],
            confidence: 0.8
        })) || [];
        const summary = {
            totalFlows: flows.length,
            criticalFlows: flows.filter((f) => f.taintLevel === 'critical').length,
            highFlows: flows.filter((f) => f.taintLevel === 'high').length,
            mediumFlows: flows.filter((f) => f.taintLevel === 'medium').length,
            lowFlows: flows.filter((f) => f.taintLevel === 'low').length,
            sourcesCount: 0,
            sinksCount: 0,
            sanitizersCount: 0
        };
        return { flows, summary, recommendations: [] };
    }
    /**
     * ÈáçË¶ÅÂ∫¶„É¨„Éô„É´„ÅÆ„Éû„ÉÉ„Éî„É≥„Ç∞
     */
    mapSeverityToLevel(severity) {
        const mapping = {
            'error': 'critical',
            'critical': 'critical',
            'high': 'high',
            'warning': 'medium',
            'medium': 'medium',
            'info': 'low',
            'low': 'low'
        };
        const key = severity?.toLowerCase();
        return (key && mapping[key] ? mapping[key] : 'medium');
    }
    /**
     * Severity„Çí„É™„Çπ„ÇØ„É¨„Éô„É´„Å´„Éû„ÉÉ„Éî„É≥„Ç∞
     */
    mapSeverityToRiskLevel(severity) {
        switch (severity?.toLowerCase()) {
            case 'error':
            case 'critical':
                return 'CRITICAL';
            case 'high':
            case 'warning':
                return 'HIGH';
            case 'medium':
                return 'MEDIUM';
            case 'info':
            case 'low':
            default:
                return 'LOW';
        }
    }
}
exports.AnalyzeCommandV8 = AnalyzeCommandV8;
